# Update these with your Supabase details from your project settings > API
# https://app.supabase.com/project/_/settings/api
NEXT_PUBLIC_SUPABASE_URL=<your-project-url>
NEXT_PUBLIC_SUPABASE_ANON_KEY=<your-anon-key>
NEXT_PUBLIC_USE_REMOTE_DEV_DB=<bool>
NEXT_PUBLIC_LOCAL_SUPABASE_URL=<supabase_url(from pnpm supabase start)>
NEXT_PUBLIC_LOCAL_ANON_KEY=<your-anon-key(from pnpm supabase start)>
NEXT_PUBLIC_LOCAL_SERVICE_ROLE=<your-service-role-key(from pnpm supabase start)>
NODE_ENV=development

ARCHIVE_PATH=<path_to_archive_folder>