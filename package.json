{
  "name": "community-archive",
  "description": "A Twitter Community Archive",
  "version": "1.0.0",
  "private": true,
  "author": "theexgenesis",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "supabase",
    "tailwindcss",
    "shadcn",
    "typescript"
  ],
  "scripts": {
    "dev": "NODE_ENV=development NEXT_PUBLIC_USE_REMOTE_DEV_DB=false next dev",
    "dev-as-prod": "NODE_ENV=production next dev",
    "dev-remote-db": "NEXT_PUBLIC_USE_REMOTE_DEV_DB=true next dev",
    "dev:importfiles": "tsx --env-file=.env scripts/import_from_files_to_db.ts",
    "dev:validateimport": "tsx --env-file=.env scripts/validate_db_import.ts",
    "dev:downloadarchive": "tsx --env-file=.env scripts/download_supabase_storage.mts",
    "dev:testconversationids": "tsx --env-file=.env scripts/testConversationIds.ts",
    "dev:generate-mock-archives": "node tests/db-insertion/fixtures/generate-mock-archives.js",
    "build": "NODE_ENV=production next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "type-check": "tsc --pretty --noEmit",
    "gen-types": "supabase gen types typescript --project-id fabxmporizzqflnftavs --schema public,dev  > src/database-types.ts",
    "pre-commit": "sh scripts/pre-commit.sh",
    "dev:gen-types": "supabase gen types typescript --local --schema public,dev  > src/database-types.ts",
    "prepare": "husky install",
    "test": "jest",
    "test:ci": "jest --ci",
    "test:server": "jest --selectProjects server --testPathPattern=src/lib",
    "test:db": "set NODE_OPTIONS=--expose-gc && jest --selectProjects server --testPathPattern=tests/db-insertion",
    "test:db:watch": "set NODE_OPTIONS=--expose-gc && jest --selectProjects server --testPathPattern=tests/db-insertion --watch",
    "test:db:coverage": "set NODE_OPTIONS=--expose-gc && jest --selectProjects server --testPathPattern=tests/db-insertion --coverage",
    "analyze": "ANALYZE=true pnpm build",
    "scratchpad": "ts-node -r tsconfig-paths/register -P ./scratchpad/tsconfig.json",
    "script": "tsx",
    "gen-api-docs": "curl https://fabxmporizzqflnftavs.supabase.co/rest/v1/ -H \"apikey: $NEXT_PUBLIC_SUPABASE_ANON_KEY\" -H \"Accept: application/openapi+json\" > public/openapi.json",
    "docker:build:process-archive": "docker build -f services/process_archive/Dockerfile -t process-archive:latest .",
    "docker:run:process-archive": "docker compose -f services/process_archive/docker-compose.yml run --rm process-archive"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^14.2.23",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.5",
    "@radix-ui/react-dropdown-menu": "^2.1.5",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-menubar": "^1.1.5",
    "@radix-ui/react-navigation-menu": "^1.2.4",
    "@radix-ui/react-popover": "^1.1.5",
    "@radix-ui/react-progress": "^1.1.1",
    "@radix-ui/react-radio-group": "^1.2.2",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "@radix-ui/react-select": "^2.1.5",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.7",
    "@scalar/nextjs-api-reference": "^0.4.108",
    "@supabase/postgrest-js": "^1.18.1",
    "@supabase/ssr": "^0.1.0",
    "@supabase/supabase-js": "^2.48.1",
    "@tanstack/react-query": "^5.66.0",
    "@tanstack/react-query-devtools": "^5.66.0",
    "@tanstack/react-table": "^8.20.6",
    "@tremor/react": "^3.18.7",
    "@vercel/analytics": "^1.4.1",
    "@zip.js/zip.js": "^2.7.57",
    "axios": "^1.8.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.7",
    "embla-carousel-react": "^8.6.0",
    "fp-ts": "^2.16.9",
    "geist": "^1.3.1",
    "lodash": "^4.17.21",
    "lucide-react": "^0.304.0",
    "next": "^14.2.28",
    "next-themes": "^0.2.1",
    "nextjs-toploader": "^1.6.12",
    "node-fetch": "^3.3.2",
    "postgres": "^3.4.7",
    "react": "18.2.0",
    "react-day-picker": "8.10.1",
    "react-dom": "18.2.0",
    "react-icons": "^5.4.0",
    "recharts": "^2.15.1",
    "supabase": "^1.226.4",
    "tailwind-merge": "^2.6.0",
    "tsx": "^4.19.2",
    "undici": "^5.28.5",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@swc/core": "^1.10.12",
    "@swc/jest": "^0.2.37",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "20.3.1",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.5",
    "adm-zip": "^0.5.16",
    "autoprefixer": "10.4.15",
    "csv-parse": "^5.6.0",
    "encoding": "^0.1.13",
    "eslint": "8.56.0",
    "eslint-config-next": "14.0.4",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.4.3",
    "msw": "^2.7.0",
    "postcss": "8.4.29",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "3.3.3",
    "tailwindcss-animate": "^1.0.7",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.1",
    "typescript": "5.1.3",
    "unzipper": "^0.12.3"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@swc/core",
      "esbuild",
      "msw",
      "supabase"
    ],
    "overrides": {
      "form-data": "4.0.4"
    }
  }
}

