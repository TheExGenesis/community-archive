{
  "name": "process-archive",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "set NODE_OPTIONS=--expose-gc && tsx --env-file=.env process.ts",
    "start": "set NODE_OPTIONS=--expose-gc && tsx --env-file=.env process_archive_upload.ts",
    "start:original": "set NODE_OPTIONS=--expose-gc && tsx --env-file=.env process.ts",
    "start:stream": "set NODE_OPTIONS=--expose-gc && tsx --env-file=.env process_stream.ts",
    "start:copy": "set NODE_OPTIONS=--expose-gc && tsx --env-file=.env process_archive_upload.ts",
    "run": "set NODE_OPTIONS=--expose-gc && tsx --env-file=.env process.ts",
    "docker:build": "cd ../../ && docker build -f services/process_archive/Dockerfile -t process-archive .",
    "docker:run": "docker compose run --rm process-archive",
    "docker:run-direct": "docker run --rm --env-file=.env -v $(pwd)/logs:/app/logs -v $(pwd)/data:/app/data process-archive",
    "docker:logs": "tail -f logs/process_archive.log",
    "docker:exec-logs": "tail -f logs/execution.log"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.2",
    "dotenv": "^16.4.5",
    "postgres": "^3.4.4",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "tsx": "^4.19.1",
    "typescript": "^5.1.3"
  }
}


